{
  "name": "compteur-rosi",
  "version": "1.0.0",
  "description": "Application de compteur pour 2 personnes avec stockage sur fichier .txt",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
